{
  "name": "real-estate-marketplace-contracts",
  "version": "1.0.0",
  "description": "Marketplace completo de real estate con 6 smart contracts desplegados en Base Sepolia",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "deploy:all": "hardhat run scripts/deploy-all.js --network baseSepolia",
    "deploy:rental": "hardhat run scripts/deploy-rental.js --network baseSepolia",
    "deploy:nft": "hardhat run scripts/deploy-nft.js --network baseSepolia",
    "deploy:escrow": "hardhat run scripts/deploy-escrow.js --network baseSepolia",
    "deploy:auction": "hardhat run scripts/deploy-auction.js --network baseSepolia",
    "deploy:insurance": "hardhat run scripts/deploy-insurance.js --network baseSepolia",
    "deploy:management": "hardhat run scripts/deploy-management.js --network baseSepolia",
    "test": "hardhat test",
    "verify:all": "hardhat run scripts/verify-all.js --network baseSepolia",
    "install": "pnpm install",
    "dev": "pnpm run compile && pnpm run deploy:all",
    "clean": "pnpm store prune"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "4.0.0",
    "@openzeppelin/contracts": "5.0.0",
    "hardhat": "2.19.0"
  },
  "dependencies": {
    "dotenv": "16.3.1"
  },
  "packageManager": "pnpm@8.0.0"
}
